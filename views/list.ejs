<%- include("header") -%>
        <div class="heading-container box">
            <button id="calendar"><i class="bi bi-calendar"></i></button>
            <div class="box" id="heading">
                <h1><%=listTitle%></h1>
            </div>
            
        </div>
        <div class="box">
            <% for(var i = 0; i < newListItems.length; i++) {%>
                <div class="item">
                    <input type="checkbox">
                    <p><%= newListItems[i] %></p>
                </div>
             <% }%>
             <form class="item" action="/" method="post">
                <input type="text" name="newItem" placeholder="New item" autocomplete="off">
                <button id="submit-btn" type="submit" name="list" value="<%= listTitle%>">+</button>
            </form>

        </div>
<%- include("footer") -%>